{
  "name": "react-isomorphic-app",
  "version": "1.0.1",
  "author": "lnyi",
  "description": "react-isomorphic-app, support SPA and MPA",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development BABEL_ENV=node NODE_PATH=./src node babelRegister ./src/server",
    "start:debug": "cross-env NODE_ENV=development BABEL_ENV=node NODE_PATH=./src node --debug babelRegister ./src/server",
    "deploy": "npm run deploy:server&&npm run deploy:client",
    "deploy:server": "cross-env NODE_ENV=production NODE_PATH=./build gulp build",
    "deploy:client": "cross-env NODE_ENV=production NODE_PATH=./build webpack --config ./config/webpack.prod.config.babel.js",
    "prod": "cross-env NODE_ENV=production NODE_PATH=./build && node build/server",
    "analyzer": "cross-env NODE_ENV=production NODE_PATH=./build webpack --config ./config/webpack.prod.config.babel.js --json>pack.json && webpack-bundle-analyzer ./pack.json ./build/static/dist",
    "lint": "eslint --env node ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lnyielea/react-isomorphic-app.git"
  },
  "bin": {
    "create-react-isomorphic-app": "./index.js"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "chokidar": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "extend": "^3.0.1",
    "extends": "0.0.4",
    "glob": "^7.1.2",
    "jade": "^1.11.0",
    "less": "^2.7.2",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "serve-favicon": "^2.4.3",
    "swig-minifier": "^0.4.4-b",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-file-map-plugin": "^1.0.6",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-dual-import": "^1.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^5.0.2",
    "css-loader": "^0.28.4",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-css-chunks-webpack-plugin": "^2.0.11",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-load-plugins": "^1.5.0",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-server-middleware": "^0.1.0"
  }
}
